{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "ES2023",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    /* Linting */
    "baseUrl": "./app",
    "paths": {
      "@/*": [
        "./*"
      ],
      "backend": [
        "../../backend/src"
      ],
      "backend/*": [
        "../../backend/src/*"
      ]
    }
  },
  "include": [
    "./app"
  ]
}