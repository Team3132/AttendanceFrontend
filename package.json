{
  "name": "AttendanceSystem",
  "version": "2.10.9",
  "main": "index.js",
  "repository": "git@github.com:Team3132/AttendanceSystem.git",
  "author": "Sebastian Pietschner <sebastian.pietschner@gmail.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bot": "pnpm run --filter bot",
    "backend": "pnpm run --filter backend",
    "frontend": "pnpm run --filter frontend",
    "build": "pnpm run -r build",
    "release": "changeset publish",
    "ci:publish": "pnpm publish -r"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "concurrently": "^8.0.0",
    "semantic-release": "^22.0.0",
    "typescript": "^5.3.3"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "pnpm": {
    "patchedDependencies": {
      "@trpc/server@11.0.0-alpha-next-2023-12-22-15-40-21.129": "patches/@trpc__server@11.0.0-alpha-next-2023-12-22-15-40-21.129.patch"
    }
  }
}
